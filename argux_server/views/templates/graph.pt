<div class="panel panel-default argux-chart" tal:attributes="data-graphid graph_id">
    <div class="panel-heading heading"><span class="title">[ Graph-Title ]</span>
        <div class="pull-right">
            <div class="btn-group">
              <button class="btn btn-default btn-xs dropdown-toggle disabled" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                <span class="caret"></span>
              </button>
              <ul class="dropdown-menu pull-right" aria-labelledby="Graph-Options">
              </ul>
            </div>
            <div class="btn-group">
                <button type="button"
                        class="btn btn-default btn-xs btn-graph-export"
                    tal:condition="graph_export=='true'"
                    tal:attributes="data-graphid graph_id">
                    <span class="glyphicon glyphicon-export"></span>
                </button>
                <button type="button"
                        class="btn btn-default btn-xs btn-graph-full"
                    tal:attributes="data-graphid graph_id">
                    <span class="glyphicon glyphicon-resize-full"></span>
                </button>
            </div>
        </div>
    </div>
    <div class="panel-body chart-body">
    </div>
</div>
