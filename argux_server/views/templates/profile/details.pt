<form id="profile-form" class="form-horizontal" action="">
    <div class="form-group">
        <label class="control-label col-xs-5" for="username">Username</label>
        <div class="col-xs-7">
        <input type="text" class="form-control" id="username" readonly value="${request.authenticated_userid}">
        </input>
        </div>
    </div>
    <div class="form-group">
        <label for="password" class="col-xs-5 control-label">Password</label>
        <div class="col-xs-7">
        <button id="password" class="form-control btn btn-primary">Reset Password</button>
        </div>
    </div>
</form>
